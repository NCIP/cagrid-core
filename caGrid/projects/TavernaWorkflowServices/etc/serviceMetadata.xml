      <ns2:Operation description="" name="Destroy">
       <ns2:inputParameterCollection/>
       <ns2:faultCollection/>
      </ns2:Operation>
      <ns2:Operation description="" name="SetTerminationTime">
       <ns2:inputParameterCollection/>
       <ns2:faultCollection/>
      </ns2:Operation>
      <ns2:Operation description="Cancels the execution of a workflow" name="cancel">
       <ns2:inputParameterCollection/>
       <ns2:faultCollection>
        <ns2:Fault description="" name="CannotCancelWorkflowFault"/>
       </ns2:faultCollection>
      </ns2:Operation>
      <ns2:Operation description="Gives a detailed status of the workflow" name="getDetailedStatus">
       <ns2:inputParameterCollection/>
       <ns2:Output qName="ns8:DetailedStatusOutputElement" dimensionality="1" isArray="true" xmlns:ns8="http://types.workflow.cagrid.nci.nih.gov/WorkflowManagementFactoryService"/>
       <ns2:faultCollection>
        <ns2:Fault description="" name="WorkflowException"/>
       </ns2:faultCollection>
      </ns2:Operation>
      <ns2:Operation description="Returns status of the workflow" name="getStatus">
       <ns2:inputParameterCollection/>
       <ns2:Output qName="ns9:WorkflowStatusElement" dimensionality="1" isArray="false" xmlns:ns9="http://types.workflow.cagrid.nci.nih.gov/WorkflowManagementFactoryService"/>
       <ns2:faultCollection>
        <ns2:Fault description="" name="WorkflowException"/>
       </ns2:faultCollection>
      </ns2:Operation>
      <ns2:Operation description="Returns the output of the workflow" name="getWorkflowOutput">
       <ns2:inputParameterCollection/>
       <ns2:Output qName="ns10:WorkflowOutputElement" dimensionality="1" isArray="false" xmlns:ns10="http://types.workflow.cagrid.nci.nih.gov/WorkflowManagementFactoryService"/>
       <ns2:faultCollection>
        <ns2:Fault description="" name="WorkflowException"/>
       </ns2:faultCollection>
      </ns2:Operation>
      <ns2:Operation description="Pause the workflow execution" name="pause">
       <ns2:inputParameterCollection/>
       <ns2:Output qName="ns11:WorkflowStatusElement" dimensionality="1" isArray="false" xmlns:ns11="http://types.workflow.cagrid.nci.nih.gov/WorkflowManagementFactoryService"/>
       <ns2:faultCollection>
        <ns2:Fault description="" name="WorkflowException"/>
        <ns2:Fault description="" name="CannotPauseWorkflowFault"/>
       </ns2:faultCollection>
      </ns2:Operation>
      <ns2:Operation description="Resumes a paused workflow" name="resume">
       <ns2:inputParameterCollection/>
       <ns2:Output qName="ns12:WorkflowStatusElement" dimensionality="1" isArray="false" xmlns:ns12="http://types.workflow.cagrid.nci.nih.gov/WorkflowManagementFactoryService"/>
       <ns2:faultCollection>
        <ns2:Fault description="" name="WorkflowException"/>
        <ns2:Fault description="" name="CannotResumeWorkflowFault"/>
       </ns2:faultCollection>
      </ns2:Operation>
      <ns2:Operation description="starts the workflow" name="start">
       <ns2:inputParameterCollection>
        <ns2:InputParameter qName="ns13:StartInputElement" dimensionality="1" index="0" isArray="false" isRequired="false" name="startInputElement" xmlns:ns13="http://types.workflow.cagrid.nci.nih.gov/WorkflowManagementFactoryService"/>
