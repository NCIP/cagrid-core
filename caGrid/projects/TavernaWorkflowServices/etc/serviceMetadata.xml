        <ns2:InputParameter qName="ns5:WMSInputElement" dimensionality="1" index="0" isArray="false" isRequired="false" name="wMSInputElement" xmlns:ns5="http://types.workflow.cagrid.nci.nih.gov/WorkflowManagementFactoryService"/>
       </ns2:inputParameterCollection>
       <ns2:Output qName="ns6:WMSOutputElement" dimensionality="1" isArray="false" xmlns:ns6="http://types.workflow.cagrid.nci.nih.gov/WorkflowManagementFactoryService"/>
       <ns2:faultCollection>
        <ns2:Fault description="" name="WorkflowException"/>
       </ns2:faultCollection>
      </ns2:Operation>
      <ns2:Operation description="" name="GetMultipleResourceProperties">
       <ns2:inputParameterCollection/>
       <ns2:faultCollection/>
      </ns2:Operation>
      <ns2:Operation description="" name="GetResourceProperty">
       <ns2:inputParameterCollection/>
       <ns2:faultCollection/>
