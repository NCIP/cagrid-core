<securityConfig xmlns="http://www.globus.org">
  <method name="getServiceSecurityMetadata">
    <auth-method>
      <none />
    </auth-method>
  </method>
  <auth-method>
    <GSITransport>
      <protection-level>
        <privacy />
      </protection-level>
    </GSITransport>
  </auth-method>
  <run-as>
    <system-identity />
  </run-as>
  <authz value="none" />
</securityConfig>

