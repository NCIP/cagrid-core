<?xml version="1.0"?>

<ivy-module version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://incubator.apache.org/ivy/schemas/ivy.xsd">

	<info organisation="jasig" module="cas-server" revision="3.2.2">
		<description homepage="http://www.ja-sig.org/products/cas/">CAS is an authentication system
			originally created by Yale University to provide a trusted way for an application to
			authenticate a user.</description>
	</info>

	<configurations>
		<!-- Public Configurations-->
		<conf name="default" visibility="public"/>
	</configurations>

	<publications>
		<artifact name="cas-server-core" type="jar"/>
	</publications>

	<dependencies>
		<dependency rev="1.1.2" org="jasig" name="person-directory"/>

		<dependency rev="1.3" org="apache" name="commons-codec"/>
		<dependency rev="3.2" org="apache" name="commons-collections"/>
		<dependency rev="2.2" org="apache" name="commons-lang"/>
		<dependency rev="1.1" org="apache" name="commons-logging"/>
		<dependency rev="2.0.8" org="apache" name="jakarta-oro"/>
		<dependency rev="1.1.2" org="apache" name="jakarta-taglibs"/>		
		<dependency rev="1.4.0" org="apache" name="santuario"/>

		<dependency rev="1.1.2" org="sun" name="jstl"/>
		<dependency rev="1.0" org="sun" name="ldapbp"/>
		<dependency rev="1.0" org="sun" name="persistence-api"/>
		<dependency rev="1.0" org="sun" name="xmldsig"/>

		<dependency rev="2.5.5" org="springframework" name="spring" conf="default->full;default->webmvc"/>
		<dependency rev="1.0.5" org="springframework" name="webflow"/>
		<dependency rev="1.2.1" org="springframework" name="ldap"/>
		<dependency rev="2.0.1" org="springframework" name="security"/>
		<dependency rev="1.0.7" org="acegisecurity" name="acegisecurity"/>		

		<dependency rev="1.2.4" org="ehcache" name="ehcache"/>
		<dependency rev="1.0" org="jdom" name="jdom"/>
		<dependency rev="2.6.9" org="ognl" name="ognl"/>
		<dependency rev="1.1b" org="internet2" name="opensaml"/>
		<dependency rev="1.5.2" org="opensymphony" name="quartz"/>
	</dependencies>
</ivy-module>
